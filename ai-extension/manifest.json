{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Sathish Code Inspector Tool",
    "version": "3",
    "description": "Written to generate feature file for automation code generator",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs",
        "sidePanel"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_title": "genai",
        "default_icon": {
            "16": "assets/images/uselogo.png",
            "32": "assets/images/uselogo.png",
            "48": "assets/images/uselogo.png",
            "128": "assets/images/uselogo.png"
        }
    },
    "icons": {
        "16": "assets/images/uselogo.png",
        "32": "assets/images/uselogo.png",
        "48": "assets/images/uselogo.png",
        "128": "assets/images/uselogo.png"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "background": {
        "service_worker": "bg.js"
    },
    "web_accessible_resources": [{
        "resources": [
            "lib/marked/marked.min.js",
            "lib/prism/prism.js",
            "lib/prism/prism-typescript.js",
            "lib/prism/prism-tomorrow.css",
            "src/content/content.js",
            "lib/css/all.min.css"
        ],
        "matches": ["<all_urls>"]
    }]
}